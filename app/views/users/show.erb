
<h1><%= @user.username %> tweets</h1>
<br></br>
<% @user.tweets.each do |tweet| %>
  <h4>Tweet content</h4>
  <h6><%= tweet.content %></h6>
  <a href="/tweets/<%= tweet.id %>/edit">Edit Content</a>
  <br></br>
  <form method="post" action="/tweets/<%= tweet.id %>/delete"> 
    <input type="submit" value="Delete">
  </form>
  <br>
  <p>------------------------</p>
<% end %>